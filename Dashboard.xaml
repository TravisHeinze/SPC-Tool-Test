<Window x:Class="SPC_Tool.Dashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        xmlns:local="clr-namespace:SPC_Tool"
        mc:Ignorable="d"
        Title="Dashboard" Height="752.586" Width="1292.187">
    <Grid Margin="0,0,-24.4,-20.2">
        <lvc:CartesianChart x:Name="spcChart1" 
                                Series="{Binding DataCollection1}" 
                                DisableAnimations="True"
                                VerticalAlignment="Stretch"
                                HorizontalAlignment="Stretch" Margin="157,114,693,354.4"
                                />
        <lvc:CartesianChart x:Name="spcChart2" 
            Series="{Binding DataCollection2}" 
            DisableAnimations="True"
            VerticalAlignment="Stretch"
            HorizontalAlignment="Stretch" Margin="750,114,100,354.4" Width="460"
                                />
        <lvc:CartesianChart x:Name="spcChart3" 
            Series="{Binding DataCollection3}" 
            DisableAnimations="True"
            VerticalAlignment="Stretch"
            HorizontalAlignment="Stretch" Margin="157,420,693,26.4" Height="274.4"
                                />
        <lvc:CartesianChart x:Name="spcChart4" 
            Series="{Binding DataCollection4}" 
            DisableAnimations="True"
            VerticalAlignment="Stretch"
            HorizontalAlignment="Stretch" Margin="750,420,100,26.4" Width="460" Height="274.4"
                                />
        <Label x:Name="Rule1_Label_1" Content="1:1" HorizontalAlignment="Left" Margin="10,114,0,0" VerticalAlignment="Top" FontSize="20" Foreground="Red"/>
        <Label x:Name="Rule2_Label_Asc_1" Content="1:2-A" HorizontalAlignment="Left" Margin="10,167,0,0" VerticalAlignment="Top" FontSize="20" RenderTransformOrigin="0.464,1.756" Foreground="Red"/>
